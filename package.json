    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^13.5.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markuplint": "^4.3.0",
    "postcss": "^8.4.35",
